{
  "name": "degen",
  "version": "0.1.1",
  "description": "Design system for mirror.xyz",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "sideEffects": [
    "src/atoms/**/*",
    "src/theme/**/*",
    "*.css.ts"
  ],
  "files": [
    "dist/**"
  ],
  "repository": "mirror-xyz/degen",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "build:site": "yarn docs:build && yarn playroom:build",
    "clean": "rimraf dist site",
    "dev": "concurrently \"yarn playroom\" \"yarn docs\"",
    "docs": "vite serve --config vite-docs.config.ts",
    "docs:build": "vite build --config vite-docs.config.ts",
    "gen:icons": "node scripts/generateIcons.js",
    "lint": "eslint . --cache",
    "lint:fix": "yarn lint --fix",
    "lint:format": "prettier --write .",
    "lint:types": "tsc --noEmit",
    "playroom": "playroom start",
    "playroom:build": "playroom build",
    "prebuild": "yarn clean && yarn setup",
    "prebuild:site": "yarn clean && yarn setup",
    "predev": "yarn setup",
    "prelint": "yarn setup",
    "pretest": "yarn setup",
    "prepare": "patch-package --patch-dir .github/patches && husky install",
    "prepublishOnly": "yarn clean && yarn build",
    "size": "size-limit",
    "setup": "yarn gen:icons",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint:fix"
    ],
    "*.{json,md,yml}": [
      "yarn lint:format"
    ]
  },
  "peerDependencies": {
    "react": "^17",
    "react-dom": "^17"
  },
  "dependencies": {
    "@vanilla-extract/css-utils": "^0.1.1",
    "@vanilla-extract/dynamic": "^2.0.0",
    "@vanilla-extract/recipes": "^0.1.0",
    "@vanilla-extract/sprinkles": "^1.2.0",
    "clsx": "^1.1.1",
    "focus-visible": "^5.2.0",
    "react-spring": "^9.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@size-limit/preset-big-lib": "^5.0.3",
    "@svgr/cli": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vanilla-extract/babel-plugin": "^1.0.1",
    "@vanilla-extract/css": "^1.4.0",
    "@vanilla-extract/vite-plugin": "^2.0.0",
    "@vanilla-extract/webpack-plugin": "^2.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.2",
    "change-case": "^4.1.2",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "dedent": "^0.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "github-slugger": "^1.4.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.2.2",
    "patch-package": "^6.4.7",
    "playroom": "0.27.4",
    "prettier": "^2.3.2",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-element-to-jsx-string": "^14.3.2",
    "react-live": "^2.3.0",
    "react-router-dom": "^5.3.0",
    "rimraf": "^3.0.2",
    "size-limit": "^5.0.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "utility-types": "^3.10.0",
    "vite": "^2.5.8",
    "vite-plugin-dts": "^0.7.0",
    "vite-plugin-mdx": "^3.5.6",
    "vite-plugin-react-pages": "^3.1.1",
    "vite-tsconfig-paths": "^3.3.14"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "size-limit": [
    {
      "path": "dist/index.cjs.js",
      "limit": "50 kB"
    },
    {
      "path": "dist/index.es.js",
      "limit": "50 kB"
    },
    {
      "path": "dist/style.css",
      "limit": "40 kB",
      "webpack": false
    }
  ]
}
